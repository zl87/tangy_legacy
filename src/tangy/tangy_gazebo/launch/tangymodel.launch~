<launch>

   <!-- Spawn a robot into Gazebo -->
   <node name="spawn_tangy_model" pkg="gazebo" type="spawn_model" args="-pause -file $(find tangymodel_description)/urdf/tangymodel.urdf -urdf -z 0 -model tangy" respawn="false" output="screen" />

   <!-- Starts the controller manager -->
   <include file="$(find tangymodel_controller_configuration_gazebo)/tangy_default_controllers.launch" />

</launch>
